{
  "kind": "collectionType",
  "collectionName": "order_managements",
  "info": {
    "singularName": "order-management",
    "pluralName": "order-managements",
    "displayName": "order_management"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "users_permissions_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "gift_card_code": {
      "type": "string"
    },
    "order_coupons": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order-coupon.order-coupon"
    },
    "order_bank_offers": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order-bank-offer.order-bank-offer"
    },
    "single_product_order": {
      "type": "dynamiczone",
      "components": [
        "details.item-list",
        "cart-order.cart-order"
      ]
    },
    "total_product_price": {
      "type": "decimal"
    },
    "is_shipping_rate": {
      "type": "boolean"
    },
    "shipping_price": {
      "type": "decimal"
    },
    "shipping_details": {
      "type": "component",
      "component": "address.delivery-address",
      "repeatable": false
    },
    "total_to_pay": {
      "type": "decimal"
    }
  }
}
